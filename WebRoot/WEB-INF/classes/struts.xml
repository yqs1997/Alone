<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
<package namespace="/" name="default" extends="struts-default" >
		<action name="begin" class="MyAction.myAction">
			<result name="success">/yuceResult.jsp</result>
		</action>
		<action name="test" class="MyAction.myAction" method="beginTest">
			<result name="success">/testResult_success.jsp</result>
			<result name="error">/testResult_error.jsp</result>
		</action>
		<action name="save" class="MyAction.myAction" method="saveAsTXT">
			<result name="success">/testResult_success.jsp</result>
		</action>
	</package>
</struts>    
